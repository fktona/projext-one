{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Project One",
  "version": "0.2.0",
  "identifier": "com.one.projectone",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Project One",
        "width": 1200,
        "height": 850,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "event": {
      "listen": true
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://github.com/fktona/projext-one/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVBOEIzOTUxMEFCNDNENUIKUldSYlBiUUtVVG1MNmhWbEJCODhubUVZNU5VOHJFeG5iWjRlSVR1cGcwcmpPTmlOalpQYWtFbTIK"
    },
    "window": {
      "all": true
    },
    "systemTray": {
      "iconPath": "assets/proj.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": false
    }
  }
}
